# Poster API Integration Settings
# Отримайте ці дані в адмін-панелі Poster
POSTER_API_TOKEN=your_poster_api_token_here
POSTER_ACCOUNT_NAME=your_account_name

# Налаштування синхронізації
POSTER_SYNC_INTERVAL_MINUTES=60
POSTER_ENABLE_AUTO_SYNC=false
POSTER_BONUS_RATE=0.05

# Приклад налаштувань:
# POSTER_API_TOKEN=abcd1234efgh5678ijkl9012mnop3456
# POSTER_ACCOUNT_NAME=myrestaurant
# POSTER_ENABLE_AUTO_SYNC=true
