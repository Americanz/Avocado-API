# Telegram Bot Logging Settings

# Загальне логування дій користувачів (True/False)
LOG_USER_ACTIONS=True

# Логування натискань кнопок (True/False)
LOG_BUTTON_CLICKS=True

# Логування команд (True/False)
LOG_COMMANDS=True

# Логування всіх повідомлень користувачів (True/False)
LOG_MESSAGES=False

# Детальне логування з додатковими параметрами (True/False)
VERBOSE_LOGGING=False

# Redis Storage для FSM (True/False)
USE_REDIS_STORAGE=False
